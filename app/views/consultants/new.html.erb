<%= form_for @consultant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>
  <%= f.label :profile %>
  <%= f.text_field :profile %>
  <br>
  <%= f.label :img %>
  <%= f.text_field :img %>
  <br>
  <%= f.label :mainimg %>
  <%= f.text_field :mainimg %>
  <br>
  <%= f.submit %>
<% end %>
